    "import gzip\n",
    "import shutil\n",
    "\n",
    "with open('foo.json', 'rb') as f_in:\n",
    "    with gzip.open('foo_compressed.json.gzip', 'wb') as f_out:\n",
    "        shutil.copyfileobj(f_in, f_out)\n",
    "    \n",
    "print('File compressed successfully.')"
